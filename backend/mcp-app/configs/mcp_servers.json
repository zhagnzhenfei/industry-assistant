{
  "servers": {
    "tavily-search": {
      "id": "tavily-search",
      "name": "Tavily search",
      "description": "Web search service for retrieving current information from the internet",
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.tavily.com/mcp/?tavilyApiKey=${TAVILY_API_KEY}"
      ],
      "timeout": 60,
      "retry_count": 3,
      "is_active": true
    }
  }
}